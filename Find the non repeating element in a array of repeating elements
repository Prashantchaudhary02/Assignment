#include <stdio.h>

void findTwoNonRepeating(int arr[], int n) {
    int XOR = 0, x = 0, y = 0;

    
    for (int i = 0; i < n; i++) {
        XOR ^= arr[i];
    }

   
    int rightmostSetBit = XOR & -XOR; 

    
    for (int i = 0; i < n; i++) {
        if (arr[i] & rightmostSetBit) {
            x ^= arr[i];
        } else {
            y ^= arr[i]; 
        }
    }

    
    printf("The two non-repeating elements are: %d and %d\n", x, y);
}

int main() {
    int arr[] = {4, 5, 4, 5, 6, 7};
    int n = sizeof(arr) / sizeof(arr[0]);

    findTwoNonRepeating(arr, n);

    return 0;
}